
import java.util.*;

public class BinaryTree {

    private Node root;
    
    public static void main(String[] args)
    {
    	String[] strings = {"harry", "ghibli", "ass", "hairy", "harry", "quit", "a", "b", "c", "d"};
    	BinaryTree tree = new BinaryTree();
    	tree.insertArray(strings);
    	
    	tree.print(tree.root);
    }
    
    private static class Node{
        String data;
        Node left;
        Node right;
        
        Node(String newdata){
            data = newdata;
            left = null;
            right = null;
        }
        
    }
    
    public void BinaryTree(){
        root = null;
    }
    
    public void insertArray(String[] newDataArray)
    {
    	for (int i = 0; i < newDataArray.length; ++i)
    	{
    		insert(newDataArray[i]);
    	}
    }
    
    
    public void print(Node n) 
    {
    	if(n == null)
    	{
    		return;
    	}
    	System.out.println(n.data);
    	print(n.left);
    	print(n.right);
    	
    }
    
    public void insert(String data){
        root = insert(root,data);
    }
    
    private Node insert(Node node, String data){
        if (node == null){
            node = new Node(data);
            System.out.println("End of tree: Making new Node");
        }
        else {
            if (data.compareTo(node.data) <= 0){
                node.left = insert(node.left, data);
                System.out.println(data + " before "+ node.data);
            }
            else {
                node.right = insert(node.right, data);
                System.out.println(data + " after " + node.data);
            }
        }
        return node;
    }
	
}
